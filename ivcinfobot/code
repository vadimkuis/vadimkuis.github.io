#–ø—Ä–æ–≤–µ—Ä–∫–∞ –ª–∏—Å—Ç–æ–≤–æ–∫
@bot.message_handler(commands=['listovki_check'])
def listovki_check(message):
    nowes = datetime.datetime.now()
    tek_day=nowes.strftime("%d.%m.%Y")
    do_day_santa="16.11.2021"
    do_day_martin = "22.11.2021"
    do_day_evroopt="28.11.2021"
    do_day_hit="28.11.2021"
    do_day_kopeechka="21.11.2021"
    do_day_ostrov="17.11.2021"
    do_day_mila="17.11.2021"
    do_day_element="15.11.2021"
    if tek_day>do_day_santa:
        webbrowser.open('https://api.telegram.org/bot1792368063:AAGqHQ1IYi37TxuJkwbLLIPALvUnn6yCVXI/sendMessage?chat_id=273298263&text=–û–±–Ω–æ–≤–∏ —Å–∫–∏–¥–∫–∏ –º–∞–≥–∞–∑–∏–Ω–∞ –°–∞–Ω—Ç–∞!',new=2)
    if tek_day>do_day_martin:
        webbrowser.open('https://api.telegram.org/bot1792368063:AAGqHQ1IYi37TxuJkwbLLIPALvUnn6yCVXI/sendMessage?chat_id=273298263&text=–û–±–Ω–æ–≤–∏ —Å–∫–∏–¥–∫–∏ –º–∞–≥–∞–∑–∏–Ω–∞ –ú–∞—Ä—Ç–∏–Ω!',new=2)
    if tek_day>do_day_evroopt:
        webbrowser.open('https://api.telegram.org/bot1792368063:AAGqHQ1IYi37TxuJkwbLLIPALvUnn6yCVXI/sendMessage?chat_id=273298263&text=–û–±–Ω–æ–≤–∏ —Å–∫–∏–¥–∫–∏ –º–∞–≥–∞–∑–∏–Ω–∞ –ï–≤—Ä–æ–æ–ø—Ç!',new=2)
    if tek_day>do_day_hit:
        webbrowser.open('https://api.telegram.org/bot1792368063:AAGqHQ1IYi37TxuJkwbLLIPALvUnn6yCVXI/sendMessage?chat_id=273298263&text=–û–±–Ω–æ–≤–∏ —Å–∫–∏–¥–∫–∏ –º–∞–≥–∞–∑–∏–Ω–∞ –•–∏—Ç!',new=2)
    if tek_day>do_day_kopeechka:
        webbrowser.open('https://api.telegram.org/bot1792368063:AAGqHQ1IYi37TxuJkwbLLIPALvUnn6yCVXI/sendMessage?chat_id=273298263&text=–û–±–Ω–æ–≤–∏ —Å–∫–∏–¥–∫–∏ –º–∞–≥–∞–∑–∏–Ω–∞ –ö–æ–ø–µ–µ—á–∫–∞!',new=2)
    if tek_day>do_day_mila:
        webbrowser.open('https://api.telegram.org/bot1792368063:AAGqHQ1IYi37TxuJkwbLLIPALvUnn6yCVXI/sendMessage?chat_id=273298263&text=–û–±–Ω–æ–≤–∏ —Å–∫–∏–¥–∫–∏ –º–∞–≥–∞–∑–∏–Ω–∞ –ú–∏–ª–∞!',new=2)
    if tek_day>do_day_element:
        webbrowser.open('https://api.telegram.org/bot1792368063:AAGqHQ1IYi37TxuJkwbLLIPALvUnn6yCVXI/sendMessage?chat_id=273298263&text=–û–±–Ω–æ–≤–∏ —Å–∫–∏–¥–∫–∏ –º–∞–≥–∞–∑–∏–Ω–∞ 5 –≠–ª–µ–º–µ–Ω—Ç!',new=2)
    else:
        bot.send_message(message.from_user.id, '–í—Å–µ —Å–∫–∏–¥–∫–∏ –µ—â—ë –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω—ã!')

#–ê–∫—Ü–∏–∏
def listovki(message):
    if message.text == "–•–∏—Ç! –≠–∫—Å–ø—Ä–µ—Å—Å":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        bot.send_message(message.from_user.id, '"–•–∏—Ç! –≠–∫—Å–ø—Ä–µ—Å—Å" —Å 15 –ø–æ 28 –Ω–æ—è–±—Ä—è ‚¨á')
        HOST = "https://hitdiscount.by"
        URL = "https://hitdiscount.by/flyer"
        HEADERS = {
            'accept': 'text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9',
            'user-agent': 'Mozilla/5.0 (Windows NT 6.1; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/90.0.4430.85 Safari/537.36'
        }

        def get_html(url, params=''):
            r = requests.get(url, headers=HEADERS, params=params)
            return r

        def get_content(html):
            soup = BeautifulSoup(html, 'html.parser')
            items = soup.find_all('figure',class_='redactor-keep-figure redactor-component')
            jobs = []

            for item in items:
                jobs.append(
                    {
                        'listovka': item.find('img').get('src')
                    }
                )
            return jobs

        def save_doc(items):
            for item in items:
                bot.send_photo(message.from_user.id, get(f"{item['listovka']}").content)
                time.sleep(1)


        def parser():
            PAGENATION = 1
            html = get_html(URL)
            if html.status_code == 200:
                jobs = []
                for page in range(0, PAGENATION):
                    html = get_html(URL, params={'page': page})
                    jobs.extend(get_content(html.text))
                    save_doc(jobs)
            else:
                bot.send_message(message.from_user.id, '–û—à–∏–±–∫–∞ —Å–∞–π—Ç –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω!')

        parser()
        keyboard.row("–ù–∞–∑–∞–¥ ‚Ü©")
        send = bot.send_message(message.from_user.id, "–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É –ù–∞–∑–∞–¥ ‚Ü©",reply_markup=keyboard)
        bot.register_next_step_handler(send, first)
    elif message.text == "–ï–≤—Ä–æ–æ–ø—Ç":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        bot.send_message(message.from_user.id, '–ï–≤—Ä–æ–æ–ø—Ç. –ö—Ä–∞—Å–Ω–∞—è —Ü–µ–Ω–∞ —Å 15 –ø–æ 28 –Ω–æ—è–±—Ä—è ‚¨á')
        HOST = 'https://mycart.by'
        URL = f'https://mycart.by/discount/8920'
        HEADERS = {
            'accept': 'text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9',
            'user-agent': 'Mozilla/5.0 (Windows NT 6.1; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/90.0.4430.85 Safari/537.36'
        }

