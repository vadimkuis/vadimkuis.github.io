    elif message.text == "–ù–∞–∑–∞–¥ ‚Ü©":
        first(message)
#–í–∞–∫–∞–Ω—Å–∏–∏
def jobs_text(message):
    keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
    HOST = "https://belmeta.com"
    URL = f"https://belmeta.com/vacansii?q={message.text}&l=–ò–≤–∞—Ü–µ–≤–∏—á–∏&sort=date"
    HEADERS = {
        'accept': 'text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9',
        'user-agent': 'Mozilla/5.0 (Windows NT 6.1; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/90.0.4430.85 Safari/537.36'
    }

    def get_html(url, params=''):
        r = requests.get(url, headers=HEADERS, params=params)
        return r

    def get_content(html):
        soup = BeautifulSoup(html, 'html.parser')
        items = soup.find_all('article', class_='job no-logo')
        jobs = []
        bot.send_message(message.from_user.id, '–í–æ—Ç —á—Ç–æ —É–¥–∞–ª–æ—Å—å –Ω–∞–π—Ç–∏ –ø–æ –≤–∞—à–µ–º—É –∑–∞–ø—Ä–æ—Å—É –∏–ª–∏ –ø–æ—Ö–æ–∂–µ–µ ‚¨á')
        for item in items:
            jobs.append(
                {
                    'link_job': HOST + item.find('h2', class_='title').find('a').get('href')
                }
            )
        return jobs

    def save_doc(items):
        for item in items:
            bot.send_message(message.from_user.id, f"{item['link_job']}")
            time.sleep(1)

    def jobs_parser():
        PAGENATION = 1
        html = get_html(URL)
        if html.status_code == 200:
            jobs = []
            for page in range(0, PAGENATION):
                html = get_html(URL, params={'page': page})
                jobs.extend(get_content(html.text))
                save_doc(jobs)
        else:
            bot.send_message(message.from_user.id, '–û—à–∏–±–∫–∞ —Å–∞–π—Ç –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω!')
    jobs_parser()
    keyboard.row("–ù–∞–∑–∞–¥ ‚Ü©")
    send = bot.send_message(message.from_user.id,"–ü–æ–∏—Å–∫ –∑–∞–∫–æ–Ω—á–µ–Ω ‚úÖ"+"\n"+"–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É –ù–∞–∑–∞–¥ ‚Ü©",reply_markup=keyboard)
    bot.register_next_step_handler(send, first)
#–ü—Ä–æ–≤–µ—Ä–∫–∞
@bot.message_handler(content_types=['text'])
def text_error(message):
    bot.send_message(message.chat.id,'–û—à–∏–±–∫–∞, —Ç–∞–∫–æ–π –∫–æ–º–º–∞–Ω–¥—ã –Ω–µ—Å—É—â–µ—Å—Ç–≤—É–µ—Ç!'+ '\n'+'–ß—Ç–æ–±—ã –æ—Ç–∫—Ä—ã—Ç—å –º–µ–Ω—é –≤–≤–µ–¥–∏—Ç–µ ‚û° /menu'+ '\n'+'–ß—Ç–æ–±—ã –ø–µ—Ä–µ–∑–∞–ø—É—Å—Ç–∏—Ç—å –±–æ—Ç–∞ –≤–≤–µ–¥–∏—Ç–µ ‚û° /start')
#polling
while True:
    try:
        bot.polling(none_stop=True,interval=0)
    except Exception as e:
        time.sleep(1)
