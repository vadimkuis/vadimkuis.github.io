            first(message)
def five(message):
    if message.text == "–ò–≤–∞—Ü–µ–≤–∏—á–∏ ‚Äì –ü–∏–Ω—Å–∫":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        bot.send_message(message.chat.id,
        '–ü–µ—Ä–∏–æ–¥–∏—á–Ω–æ—Å—Ç—å –∫—É—Ä—Å–∏—Ä–æ–≤–∞–Ω–∏—è –∞–≤—Ç–æ–±—É—Å–∞:' + '\n' + '–ï–∂–µ–¥–Ω–µ–≤–Ω–æ' + '\n' + '–í—Ä–µ–º—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Å –∞–≤—Ç–æ—Å—Ç–∞–Ω—Ü–∏–∏:' + '\n' + '7-40' + '\n' + '–ü–µ—Ä–∏–æ–¥–∏—á–Ω–æ—Å—Ç—å –∫—É—Ä—Å–∏—Ä–æ–≤–∞–Ω–∏—è –∞–≤—Ç–æ–±—É—Å–∞:' + '\n' + '–ß—Ç,–≤—Å' + '\n' + '–í—Ä–µ–º—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Å –∞–≤—Ç–æ—Å—Ç–∞–Ω—Ü–∏–∏:' + '\n' + '15-30' + '\n' + '–î–∞–Ω–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –Ω–æ—Å–∏—Ç —Å–ø—Ä–∞–≤–æ—á–Ω—ã–π —Ö–∞—Ä–∞–∫—Ç–µ—Ä.' + '\n' + '–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ —Ä–∞—Å–ø–∏—Å–∞–Ω–∏–∏ –º–∞—Ä—à—Ä—É—Ç–æ–≤ –º–æ–∂–Ω–æ —É—Ç–æ—á–Ω–∏—Ç—å –Ω–∞  –∞–≤—Ç–æ—Å—Ç–∞–Ω—Ü–∏–∏ –ò–≤–∞—Ü–µ–≤–∏—á–∏ –ø–æ —Ç–µ–ª–µ—Ñ–æ–Ω—É 80164524950.')
        keyboard.row("–ù–∞–∑–∞–¥ ‚Ü©")
        send = bot.send_message(message.from_user.id, "–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É –ù–∞–∑–∞–¥ ‚Ü©", reply_markup=keyboard)
        bot.register_next_step_handler(send, five)
    elif message.text == "–õ–∏–¥–∞ ‚Äì –ü–∏–Ω—Å–∫":
