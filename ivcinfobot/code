            for item in items:
                bot.send_message(message.from_user.id,"–í—Ä–µ–º—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–∏—è:" + "\n" + f"{item['trains']}" + "\n" + "–í—Ä–µ–º—è –≤ –ø—É—Ç–∏:" + "\n" + f"{item['time_trains']}" + "\n" + "–í—Ä–µ–º—è –ø—Ä–∏–±—ã—Ç–∏—è:" + "\n" + f"{item['arrival_trains']}" + "\n" + "–ü–µ—Ä–∏–æ–¥–∏—á–Ω–æ—Å—Ç—å –∫—É—Ä—Å–∏—Ä–æ–≤–∞–Ω–∏—è:" + "\n" + f"{item['run_day']}"+ "\n" + "–°—Ç–∞–Ω—Ü–∏—è –ø—Ä–∏–±—ã—Ç–∏—è:" + "\n" + f"{item['stops']}")
