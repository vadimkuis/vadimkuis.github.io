@bot.message_handler(commands=['adm_cmd'])
def start(message):
    bot.send_message(message.chat.id, "/sms95 - —Ä–∞—Å—Å—ã–ª–∫–∞ —Å–º—Å –≤—Å–µ–º –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è–º"+"\n"+"/listovki_check - –ø—Ä–æ–≤–µ—Ä–∫–∞ –∞–∫—Ç—É–∞–ª—å–Ω–æ—Å—Ç–∏ –ª–∏—Å—Ç–æ–≤–æ–∫")
#–û–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å
def feed_text(message):
    keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
    feedback_text = message.text
    connect = sqlite3.connect('users.db')
    cursor = connect.cursor()
    dat = cursor.execute(f"SELECT datetime('now','localtime')")
    dat= cursor.fetchone()
    date=str(dat)
    cursor.execute("""CREATE TABLE IF NOT EXISTS feedback(
                        id INTEGER,
                        username TEXT,
                        message TEXT,
                        date TEXT
            )""")
    user_id = [message.chat.id, message.from_user.username,feedback_text,date]
    cursor.execute("INSERT INTO feedback VALUES(?,?,?,?);", user_id)
    connect.commit()
    bot.send_message(message.chat.id,"–í–∞—à–µ –æ–±—Ä–∞—â–µ–Ω–∏–µ —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–æ!")
    keyboard.row("–ù–∞–∑–∞–¥ ‚Ü©")
    send = bot.send_message(message.from_user.id, "–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É –ù–∞–∑–∞–¥ ‚Ü©", reply_markup=keyboard)
    bot.register_next_step_handler(send, first)
#–ú–µ–Ω—é
@bot.message_handler(commands=['menu'])
def first(message):
    tg_analytic.statistics(message.chat.id, message.text)
    #–ú–µ–Ω—é –∫–Ω–æ–ø–∫–∏
    key = telebot.types.ReplyKeyboardMarkup(True, False)
    key.row('–ò—Å—Ç–æ—Ä–∏—è –≥–æ—Ä–æ–¥–∞ üèó', '–ü–æ—Å–ª–µ–¥–Ω–∏–µ –Ω–æ–≤–æ—Å—Ç–∏ üóû')
    key.row('–ü–æ–≥–æ–¥–∞ üå§', '–ö—É—Ä—Å—ã –≤–∞–ª—é—Ç üí≤')
    key.row('–í–∞–∫–∞–Ω—Å–∏–∏ üíº','–ú–∞–≥–∞–∑–∏–Ω—ã üè™')
    key.row('–ö–∞—Ñ–µ ‚òï', '–†–µ—Å—Ç–æ—Ä–∞–Ω—ã üçΩ')
    key.row('–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ –∞–≤—Ç–æ–±—É—Å–æ–≤ üöå', '–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ –∂–¥ üöà')
    key.row('–¢–∞–∫—Å–∏ üöï', '–ê–ó–° ‚õΩ')
    key.row('–ê–ø—Ç–µ–∫–∏ üíä','–ì–æ—Å—Ç–∏–Ω–∏—Ü—ã üè®')
    key.row('–°–ø–æ—Ä—Ç–∏–≤–Ω—ã–µ –æ–±—ä–µ–∫—Ç—ã üèü','–û–ø–ª–∞—Ç–∞ —É—Å–ª—É–≥ üí≥')
    key.row('–°–≤—è–∑—å —Å —Ä–∞–∑—Ä–∞–±–æ—Ç—á–∏–∫–æ–º üó£')
    send = bot.send_message(message.from_user.id, "–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é", reply_markup=key)
    bot.register_next_step_handler(send, second)
#–†–∞–∑–¥–µ–ª—ã –º–µ–Ω—é
