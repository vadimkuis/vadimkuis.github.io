    elif message.text == "–ì–µ—Ä–± –∏ —Ñ–ª–∞–≥":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        bot.send_photo(message.chat.id, get("http://ivacevichi.brest-region.gov.by/images/stories/area_files/ivacevichi/images/12104-1.jpg").content,'–ì–µ—Ä–± –≥–æ—Ä–æ–¥–∞ –ò–≤–∞—Ü–µ–≤–∏—á–∏ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è–µ—Ç —Å–æ–±–æ–π —â–∏—Ç, —Å–∫–æ—à–µ–Ω–Ω—ã–π —Å–ª–µ–≤–∞ –Ω–∞ –∑–µ–ª–µ–Ω—å –∏ –∑–æ–ª–æ—Ç–æ. –í –∑–µ–ª–µ–Ω–∏ –≥–µ—Ä–± ¬´–ì–æ–∑–¥–∞–≤–∞¬ª ‚Äî —Å–µ—Ä–µ–±—Ä—è–Ω–∞—è –¥–≤–æ–π–Ω–∞—è –ª–∏–ª–∏—è —Å –∑–æ–ª–æ—Ç—ã–º –∫–æ–ª—å—Ü–æ–º. –ì–µ—Ä–± –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω–æ —É—Ç–≤–µ—Ä–∂–¥—ë–Ω —É–∫–∞–∑–æ–º –ø—Ä–µ–∑–∏–¥–µ–Ω—Ç–∞ –†–µ—Å–ø—É–±–ª–∏–∫–∏ –ë–µ–ª–∞—Ä—É—Å—å ‚Ññ 659 –æ—Ç 2 –¥–µ–∫–∞–±—Ä—è 2008 –≥–æ–¥–∞.')
        bot.send_photo(message.chat.id, get("http://ivacevichi.brest-region.gov.by/images/stories/area_files/ivacevichi/images/12104-2.jpg").content,'–§–ª–∞–≥ –∏–º–µ–µ—Ç —Ñ–æ—Ä–º—É –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫–∞, —Ä–∞–∑–¥–µ–ª—ë–Ω–Ω–æ–≥–æ –ø–æ –¥–∏–∞–≥–æ–Ω–∞–ª–∏ —Å –ø—Ä–∞–≤–æ–≥–æ –Ω–∏–∂–Ω–µ–≥–æ —É–≥–ª–∞ –∫ –ª–µ–≤–æ–º—É –≤–µ—Ä—Ö–Ω–µ–º—É —É–≥–ª—É. –ü—Ä–∞–≤–∞—è –≤–µ—Ä—Ö–Ω—è—è —á–∞—Å—Ç—å —Ñ–ª–∞–≥–∞ ‚Äì –∑–µ–ª—ë–Ω–æ–≥–æ —Ü–≤–µ—Ç–∞, –ª–µ–≤–∞—è –Ω–∏–∂–Ω—è—è ‚Äì –∑–æ–ª–æ—Ç–∏—Å—Ç–æ-–∂—ë–ª—Ç–æ–≥–æ. –í –∑–µ–ª—ë–Ω–æ–π —á–∞—Å—Ç–∏ —Ñ–ª–∞–≥–∞ –≤ –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É —Ä–∞–∑–º–µ—â–µ–Ω–∞ —Å–µ—Ä–µ–±—Ä—è–Ω–∞—è —Å –∑–æ–ª–æ—Ç—ã–º –æ–±–æ–¥–∫–æ–º –¥–≤–æ–π–Ω–∞—è –ª–∏–ª–∏—è. –¶–≤–µ—Ç–∞ –∏ —Å–∏–º–≤–æ–ª —Ñ–ª–∞–≥–∞ –∏–¥–µ–Ω—Ç–∏—á–Ω—ã —Ü–≤–µ—Ç–∞–º –∏ —Å–∏–º–≤–æ–ª—É –≥–µ—Ä–±–∞ –≥–æ—Ä–æ–¥–∞ –ò–≤–∞—Ü–µ–≤–∏—á–∏.')
        send = bot.send_message(message.from_user.id, "–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É –ù–∞–∑–∞–¥ ‚Ü©",reply_markup=keyboard)
        bot.register_next_step_handler(send, third)
    elif message.text == "–ì–∏–º–Ω":
