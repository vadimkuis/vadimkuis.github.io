        bot.send_photo(message.chat.id, get("https://tourism.rooivacevichi.gov.by/files/00181/obj/110/36993/img/%D0%9F%D0%B0%D0%BC%D1%8F%D1%82%D0%BD%D1%8B%D0%B9%20%D0%B7%D0%BD%D0%B0%D0%BA%20%D0%BA%20490-%D0%BB%D0%B5%D1%82%D0%B8%D1%8E.jpg").content)
