#–°—Ç–∞—Ä—Ç

@bot.message_handler(commands=['start'])
def start(message):
#–ü–æ–¥–∫–ª –ë–î
    now = datetime.datetime.now()
    connect = sqlite3.connect('users.db')
    cursor = connect.cursor()
    dat = cursor.execute(f"SELECT datetime('now','localtime')")
    dat= cursor.fetchone()
    date=str(dat)
    cursor.execute("""CREATE TABLE IF NOT EXISTS login_id(
                   id INTEGER,
                   username TEXT,
                   first TEXT,
                   last_name TEXT,
                   date TEXT
    )""")
#–ø–æ–¥—Å—á–µ—Ç –ø–æ–¥–ø–∏—Å—á–∏–∫–æ–≤
    kol=cursor.execute("SELECT count(*) FROM login_id")
    kol=cursor.fetchone()
    connect.commit()
#–ø—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –ø–æ–≤—Ç–æ—Ä
    people_id = message.chat.id
    cursor.execute(f"SELECT id FROM login_id WHERE id = {people_id}")
    data = cursor.fetchone()
    if data is None:
#–¥–æ–± –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª
        user_id = [message.chat.id, message.from_user.username, message.from_user.first_name, message.from_user.last_name, date]
        cursor.execute("INSERT INTO login_id VALUES(?,?,?,?,?);", user_id)
        connect.commit()
#–ü—Ä–∏–≤–µ—Å—Ç–≤–∏–µ
    tg_analytic.statistics(message.chat.id, message.text)
    bot.send_message(message.chat.id, message.from_user.first_name + ', –¥–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å üëã' + '\n'+ '–í –ø–µ—Ä–≤—ã–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω—ã–π –±–æ—Ç ü§ñ –≥. –ò–≤–∞—Ü–µ–≤–∏—á–∏ ¬Æ'+ '\n'+ f'{now.strftime("–¢–µ–∫—É—â–∞—è –¥–∞—Ç–∞: %d.%m.%Y, —Ç–µ–∫—É—â–µ–µ –≤—Ä–µ–º—è: %H:%M")}'+ '\n' + '–ß—Ç–æ–±—ã –æ—Ç–∫—Ä—ã—Ç—å –º–µ–Ω—é –≤–≤–µ–¥–∏—Ç–µ –∫–æ–º–∞–Ω–¥—É ‚û° /menu'+ '\n' + f'üîπ {kol[0]}' + ' —á–µ–ª–æ–≤–µ–∫ üë• –ø–æ–¥–ø–∏—Å–∞–Ω–æ –Ω–∞ –±–æ—Ç–∞ ‚úî'+'\n'+'–î–µ–ª–∏—Ç–µ—Å—å —Å—Å—ã–ª–∫–æ–π –Ω–∞ –±–æ—Ç–∞ —Å –¥—Ä—É–∑—å—è–º–∏ ü§ù')

#–æ—Ç–ø—Ä–∞–≤–∫–∞ —Å–º—Å –≤—Å–µ–º
@bot.message_handler(commands=['sms95'])
def text(message):
        con = lite.connect('users.db')
        with con:
            cur = con.cursor()
            cur.execute("SELECT * FROM login_id")
            while True:
                row = cur.fetchone()
                if row == None:
                    break
                webbrowser.open(f'https://api.telegram.org/bot1792368063:AAGqHQ1IYi37TxuJkwbLLIPALvUnn6yCVXI/sendMessage?chat_id={row[0]}&text=–í—Å–µ–º –ø—Ä–∏–≤–µ—Ç üëã –ë–æ—Ç –æ–±–Ω–æ–≤–ª—ë–Ω ü•≥ –î–µ–ª–∏—Ç–µ—Å—å —Å—Å—ã–ª–∫–æ–π –Ω–∞ –±–æ—Ç–∞ —Å –≤–∞—à–∏–º–∏ –∑–Ω–∞–∫–æ–º—ã–º–∏ –∏ –¥—Ä—É–∑—å—è–º–∏ ‚û° https://t.me/Ivatsevichi_Info_Bot',new=2)
