                    time.sleep(1)

            def apteka_parser():
                PAGENATION = 1
                html = get_html(URL)
                if html.status_code == 200:
                    jobs = []
                    for page in range(0, PAGENATION):
                        html = get_html(URL, params={'page': page})
                        jobs.extend(get_content(html.text))
                        save_doc(jobs)
                else:
                    bot.send_message(message.from_user.id, '–û—à–∏–±–∫–∞ —Å–∞–π—Ç —Å –∞–ø—Ç–µ–∫–∞–º–∏ –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω!')

