#–ü–æ–µ–∑–¥–∞
def seven(message):
    if message.text == "–£—Ç—Ä–æ (06:00 - 12:00)":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        bot.send_message(message.chat.id, "–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ –ø–æ–µ–∑–¥–æ–≤" + "\n" + "–ò–≤–∞—Ü–µ–≤–∏—á–∏ ‚Äî –ë—Ä–µ—Å—Ç —É—Ç—Ä–æ–º ‚¨á")
        HOST = 'https://t.rasp.yandex.by'
        URL = f'https://t.rasp.yandex.by/train/ivatsevichi-station--brest?departure%5B0%5D=morning'
        HEADERS = {
            'accept': 'text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9',
            'user-agent': 'Mozilla/5.0 (Windows NT 6.1; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/90.0.4430.85 Safari/537.36'
        }

        def get_html(url, params=''):
            r = requests.get(url, headers=HEADERS, params=params)
            return r

        def get_content(html):
            soup = BeautifulSoup(html, 'html.parser')
            items = soup.find_all('article', class_='SearchSegment SearchSegments__segment')
            jobs = []

            for item in items:
                jobs.append(
                    {
                        'trains': item.find("div", class_="TimeAndStations__departureTime").get_text(strip=True),
                        'time_trains': item.find("div", class_="TimeAndStations__duration").get_text(strip=True),
                        'arrival_trains': item.find("td", class_="TimeAndStations__arrivalTime").get_text(strip=True),
                        'run_day': item.find("div", class_="SegmentRunDays SearchSegment__runDays").find('span').get_text(strip=True),
                        'stops': item.find("td", class_="TimeAndStations__arrivalStation").find('a').get_text(strip=True)
                    }
                )
            return jobs

        def save_doc(items):
            for item in items:
                bot.send_message(message.from_user.id,"–í—Ä–µ–º—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–∏—è:" + "\n" + f"{item['trains']}" + "\n" + "–í—Ä–µ–º—è –≤ –ø—É—Ç–∏:" + "\n" + f"{item['time_trains']}" + "\n" + "–í—Ä–µ–º—è –ø—Ä–∏–±—ã—Ç–∏—è:" + "\n" + f"{item['arrival_trains']}" + "\n" + "–ü–µ—Ä–∏–æ–¥–∏—á–Ω–æ—Å—Ç—å –∫—É—Ä—Å–∏—Ä–æ–≤–∞–Ω–∏—è:" + "\n" + f"{item['run_day']}"+ "\n" + "–°—Ç–∞–Ω—Ü–∏—è –ø—Ä–∏–±—ã—Ç–∏—è:" + "\n" + f"{item['stops']}")
                time.sleep(1)

