def six(message):
    if message.text == "–£—Ç—Ä–æ(06:00 - 12:00)":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        bot.send_message(message.chat.id, "–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ —ç–ª–µ–∫—Ç—Ä–∏—á–µ–∫"+ "\n" + "–ò–≤–∞—Ü–µ–≤–∏—á–∏ ‚Äî –ë–∞—Ä–∞–Ω–æ–≤–∏—á–∏-–ü–æ–ª–µ—Å—Å–∫–∏–µ —É—Ç—Ä–æ–º ‚¨á")
        HOST = 'https://t.rasp.yandex.by'
        URL = f'https://t.rasp.yandex.by/suburban/ivatsevichi-station--baranavichy?departure%5B0%5D=morning'
        HEADERS = {
            'accept': 'text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9',
            'user-agent': 'Mozilla/5.0 (Windows NT 6.1; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/90.0.4430.85 Safari/537.36'
        }

        def get_html(url, params=''):
            r = requests.get(url, headers=HEADERS, params=params)
            return r

        def get_content(html):
            soup = BeautifulSoup(html, 'html.parser')
            items = soup.find_all('article', class_='SearchSegment SearchSegments__segment')
            jobs = []

            for item in items:
                jobs.append(
                    {
                        'trains': item.find("div", class_="TimeAndStations__departureTime").get_text(strip=True),
                        'time_trains': item.find("div", class_="TimeAndStations__duration").get_text(strip=True),
                        'arrival_trains': item.find("td", class_="TimeAndStations__arrivalTime").get_text(strip=True),
                        'run_day': item.find("div", class_="SegmentRunDays SearchSegment__runDays").find('span').get_text(strip=True),
                        'stops': item.find("div", class_="SegmentStops SearchSegment__stops").get_text(strip=True)
                    }
                )
            return jobs

        def save_doc(items):
            for item in items:
                bot.send_message(message.from_user.id, "–í—Ä–µ–º—è –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–∏—è:" + "\n" + f"{item['trains']}" + "\n" + "–í—Ä–µ–º—è –≤ –ø—É—Ç–∏:" + "\n" + f"{item['time_trains']}" + "\n" + "–í—Ä–µ–º—è –ø—Ä–∏–±—ã—Ç–∏—è:" + "\n" + f"{item['arrival_trains']}" + "\n" + "–ü–µ—Ä–∏–æ–¥–∏—á–Ω–æ—Å—Ç—å –∫—É—Ä—Å–∏—Ä–æ–≤–∞–Ω–∏—è:" + "\n" + f"{item['run_day']}" + "\n" + "–û—Å—Ç–∞–Ω–æ–≤–∫–∏:" + "\n" + f"{item['stops']}")
                time.sleep(1)

        def parser_train_morning():
            PAGENATION = 1
            html = get_html(URL)
            if html.status_code == 200:
                jobs = []
                bot.send_message(message.from_user.id, '–ò–¥—ë—Ç –ø–æ–∏—Å–∫ ‚¨á')
                for page in range(0, PAGENATION):
                    html = get_html(URL, params={'page': page})
                    jobs.extend(get_content(html.text))
                    save_doc(jobs)
            else:
                bot.send_message(message.from_user.id, '–û—à–∏–±–∫–∞, —Å–∞–π—Ç –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω!')

        parser_train_morning()
        keyboard.row("–ù–∞–∑–∞–¥ ‚Ü©")
        send = bot.send_message(message.from_user.id, "–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É –ù–∞–∑–∞–¥ ‚Ü©", reply_markup=keyboard)
        bot.register_next_step_handler(send, first)
    elif message.text == "–î–µ–Ω—å(12:00 - 18:00)":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        bot.send_message(message.chat.id, "–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ —ç–ª–µ–∫—Ç—Ä–∏—á–µ–∫"+ "\n" + "–ò–≤–∞—Ü–µ–≤–∏—á–∏ ‚Äî –ë–∞—Ä–∞–Ω–æ–≤–∏—á–∏-–ü–æ–ª–µ—Å—Å–∫–∏–µ –¥–Ω—ë–º ‚¨á")
