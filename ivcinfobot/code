        bot.register_next_step_handler(send, first)
    elif message.text == "–í–µ—á–µ—Ä(18:00 - 00:00)":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        bot.send_message(message.chat.id, "–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ —ç–ª–µ–∫—Ç—Ä–∏—á–µ–∫"+ "\n" + "–ò–≤–∞—Ü–µ–≤–∏—á–∏ ‚Äî –ë–∞—Ä–∞–Ω–æ–≤–∏—á–∏-–ü–æ–ª–µ—Å—Å–∫–∏–µ –≤–µ—á–µ—Ä–æ–º ‚¨á")
        HOST = 'https://t.rasp.yandex.by'
        URL = f'https://t.rasp.yandex.by/suburban/ivatsevichi-station--baranavichy?departure%5B0%5D=evening'
