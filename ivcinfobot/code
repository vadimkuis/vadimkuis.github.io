#–ü–æ–≥–æ–¥–∞
def weather_today(message):
    if message.text == "–£—Ç—Ä–æ–º":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        weather_ivatcevichi = requests.get('https://rusmeteo.net/weather/ivacevichi/3days')
        html_tree = lxml.html.document_fromstring(weather_ivatcevichi.text)
        day = html_tree.xpath("/html/body/div/div[2]/div[2]/div[1]/div[1]/div/div[2]/div[1]/p")
        weather = html_tree.xpath("/html/body/div[1]/div[2]/div[2]/div[1]/div[1]/div/div[2]/table/tbody/tr[2]/th[4]")
        weather_title = html_tree.xpath("/html/body/div[1]/div[2]/div[2]/div[1]/div[1]/div/div[2]/table/tbody/tr[2]/th[3]")
        bot.send_message(message.chat.id,f'–°–µ–≥–æ–¥–Ω—è ' + f'{day[0].text}' + ' –ø–æ–≥–æ–¥–∞ –≤ –ò–≤–∞—Ü–µ–≤–∏—á–∞—Ö —É—Ç—Ä–æ–º:' + '\n' + f'{weather[0].text}'+ '\n' + f'{weather_title[0].text}')
        send = bot.send_message(message.from_user.id, "–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É –ù–∞–∑–∞–¥ ‚Ü©", reply_markup=keyboard)
        bot.register_next_step_handler(send, weather_today)
    elif message.text == "–î–Ω—ë–º":
        keyboard = telebot.types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=False)
        weather_ivatcevichi = requests.get('https://rusmeteo.net/weather/ivacevichi/3days')
