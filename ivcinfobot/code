                time.sleep(1)

        def parser_train_day():
            PAGENATION = 1
            html = get_html(URL)
            if html.status_code == 200:
                jobs = []
                bot.send_message(message.from_user.id, '–ò–¥—ë—Ç –ø–æ–∏—Å–∫ ‚¨á')
